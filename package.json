{
    "name": "contactmanager",
    "version": "2.1.11",
    "description": "The three musketeers, all in one pack in the form of a boilerplate app",
    "homepage": "https://arif.bannehasan/contactmanager",
    "bugs": {
        "url": "https://github.com/arif.bannehasan/contactmanager/issues",
        "email": "arif.bannehasan@gmail.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/arif.bannehasan/contactmanager.git"
    },
    "scripts": {
        "prestart": "npm run docker-mongo",
        "clean": "node_modules/.bin/rimraf dist",
        "start": "npm run serve",
        "build": "npm run build-ts && npm run lint",
        "serve": "node dist/server.js",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "docker-mongo": "docker-compose -f mongo-stack.yml up -d",
        "docker-mongo-down": "docker-compose -f mongo-stack.yml down",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
        "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "copy-static-assets": "ts-node copyStaticAssets.ts",
        "debug": "nodemon --inspect=9229 dist/server.js",
        "serve-debug": "",
        "watch-debug": "concurrently \"npm run watch-ts\" \"npm run serve-debug\"",
        "webpack-build": "node_modules/.bin/webpack --mode=development --config ./config/webpack.config.js",
        "webpack-build-watch": "npm run clean && node_modules/.bin/webpack --mode=development --watch --progress --config ./config/webpack.config.js",
        "webpack-debug": "nodemon --inspect=9229 dist/compiled.js",
        "client-start": "react-scripts start",
        "client-build": "react-scripts build",
        "client-test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "react-scripts build",
        "deploy": "gh-pages -d build"
    },
    "config": {
        "title": "Contact Manager",
        "theme": {
            "primaryColor": "blue",
            "secondaryColor": "red",
            "type": "light",
            "dense": false
        }
    },
    "dependencies": {
        "@material-ui/core": "^4.8.3",
        "@material-ui/icons": "^4.5.1",
        "@material-ui/styles": "^4.8.2",
        "@sentry/browser": "^5.11.0",
        "async": "^3.1.0",
        "bcrypt-nodejs": "^0.0.3",
        "bluebird": "^3.5.5",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "connect-mongo": "^3.0.0",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.16.2",
        "express-validator": "^6.2.0",
        "fbgraph": "^1.4.4",
        "firebase": "^7.6.2",
        "lodash": "^4.17.15",
        "lodash.camelcase": "^4.3.0",
        "lusca": "^1.6.1",
        "mdi-material-ui": "^6.10.0",
        "moment": "^2.24.0",
        "mongoose": "^5.7.5",
        "nodemailer": "^6.3.0",
        "passport": "^0.4.0",
        "passport-facebook": "^3.0.0",
        "passport-local": "^1.0.0",
        "pug": "^2.0.4",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.3.0",
        "react-swipeable-views": "^0.13.1",
        "reading-time": "^1.1.3",
        "swagger-express-ts": "^1.1.0",
        "swagger-ui-dist": "*",
        "typeface-roboto": "^0.0.75",
        "validate.js": "^0.13.1",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@types/async": "^3.0.2",
        "@types/bcrypt-nodejs": "^0.0.30",
        "@types/bluebird": "^3.5.27",
        "@types/body-parser": "^1.17.1",
        "@types/chai": "^4.2.3",
        "@types/compression": "^1.0.1",
        "@types/concurrently": "^4.1.0",
        "@types/connect-mongo": "^3.0.0",
        "@types/errorhandler": "^0.0.32",
        "@types/eslint": "^6.1.1",
        "@types/express": "^4.17.1",
        "@types/express-flash": "0.0.1",
        "@types/express-session": "^1.15.14",
        "@types/jest": "^24.0.23",
        "@types/jquery": "^3.3.31",
        "@types/lodash": "^4.14.141",
        "@types/lusca": "^1.6.1",
        "@types/mongoose": "^5.5.18",
        "@types/node": "^12.7.8",
        "@types/node-sass": "^4.11.0",
        "@types/nodemailer": "^6.2.1",
        "@types/passport": "^1.0.1",
        "@types/passport-facebook": "^2.1.9",
        "@types/passport-google-oauth": "^1.0.41",
        "@types/passport-local": "^1.0.33",
        "@types/pug": "^2.0.4",
        "@types/request": "^2.48.3",
        "@types/request-promise": "^4.1.44",
        "@types/shelljs": "^0.8.5",
        "@types/supertest": "^2.0.8",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "chai": "^4.2.0",
        "concurrently": "^5.0.0",
        "dotenv-webpack": "^7.0.3",
        "eslint": "^6.4.0",
        "gh-pages": "^2.2.0",
        "jest": "^24.9.0",
        "node-sass": "^4.14.0",
        "nodemon": "^1.19.2",
        "passport-google-oauth": "^2.0.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^2.6.3",
        "shelljs": "^0.8.3",
        "source-map-loader": "^1.1.3",
        "supertest": "^4.0.2",
        "ts-jest": "^24.1.0",
        "ts-loader": "^4.4.2",
        "ts-node": "^8.4.1",
        "typescript": "^3.6.3",
        "webpack": "^4.41.2",
        "webpack-cli": "^4.7.2"
    },
    "private": true,
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
